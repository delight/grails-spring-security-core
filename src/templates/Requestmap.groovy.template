package ${packageName}

class ${requestmapClassName} {

	String url
	String configAttribute

	static mapping = {
		cache true
	}

	static constraints = {
		url blank: false, unique: true
		configAttribute blank: false
	}

	def springSecurityService
	
	def afterInsert() {
		springSecurityService.clearCachedRequestmaps()
	}
	
	def afterUpdate() {
		springSecurityService.clearCachedRequestmaps()
	}
	
	def afterDelete() {
		springSecurityService.clearCachedRequestmaps()
	}

}
